filebeat.config:
  modules:
    path: ${path.config}/modules.d/*.yml
    reload.enabled: false

filebeat.inputs:
- type: log
  paths:
{% for paths in pipelinematch.paths %}
  {% for item in paths %}
  - {{ item }}
  {% endfor %}
{% endfor %}

  tags:
{% for tags in pipelinematch.tags %}
  {% for item in tags %}
  - {{ item }}
  {% endfor %}
{% endfor %}

output.elasticsearch:
  hosts: {{ elastic_hosts | to_yaml }}
  bulk_max_size: {{ bulk_max_size }}
